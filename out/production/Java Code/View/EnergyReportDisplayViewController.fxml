      <Button fx:id="DonwloadButton" layoutX="488.0" layoutY="333.0" mnemonicParsing="false" text="Donwload">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label fx:id="Date1Label" layoutX="226.0" layoutY="290.0" text="Date1">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="Date2Label" layoutX="336.0" layoutY="290.0" text="Date2">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
